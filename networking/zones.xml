<entry name="{{ template_name }}">
  <description>topology or site type recommended template elements</description>
  <settings>
    <default-vsys>vsys1</default-vsys>
  </settings>
  <config>
    <devices>
      <entry name="localhost.localdomain">
        <network>
          <profiles>
            <interface-management-profile>
              <entry name="ping-only">
                <ping>yes</ping>
              </entry>
              <entry name="ping-and-response">
                <ping>yes</ping>
                <response-pages>yes</response-pages>
              </entry>
              <entry name="full-mgmt">
                <https>yes</https>
                <ssh>yes</ssh>
                <ping>yes</ping>
              </entry>
            </interface-management-profile>
          </profiles>
        </network>
      </devices>
    </config>
  </entry>